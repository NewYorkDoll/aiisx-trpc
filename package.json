{
  "name": "aiisx",
  "version": "10.18.0",
  "private": true,
  "scripts": {
    "dx:next": "next dev",
    "dx": "run-p dx:* --print-label",
    "dev": "run-s migrate-dev db-seed && pnpm dx:next",
    "dev:server": "tsx watch src/server",
    "db-seed": "prisma db seed",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "build": "next build",
    "start": "export PORT=3004 && next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@nextui-org/react": "^1.0.0-beta.12",
    "@prisma/client": "^4.14.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "@types/node-cron": "^3.0.7",
    "@urql/core": "^4.0.4",
    "ahooks": "^3.7.6",
    "axios": "^1.4.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.9",
    "next": "^13.2.1",
    "next-themes": "^0.2.1",
    "node-cron": "^3.0.2",
    "node-schedule": "^2.1.1",
    "octokit": "^2.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "timeago-react": "^3.0.6",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.0",
    "@playwright/test": "^1.26.1",
    "@types/node": "^20.1.3",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.7",
    "prisma": "^4.14.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.3",
    "typescript": "^5.0.4",
    "vite": "^4.1.2",
    "vitest": "^0.32.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
